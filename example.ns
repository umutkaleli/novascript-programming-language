START

(>> This program prints fibonacci serie using
	while loop  <<)
	
int n1 = 0;
int n2 = 1;
int n3;
int i = 2;
int count = 20;

while(i < count){
	n3 = n1 + n2;
	print(" " + n3);
	n1 = n2;
	n2 = n3;
	i = i + 1;
}	
