START

(>> This program checks if a number is even or not
	then prints that variable is even or not <<)
	
int a = 8;

bool isEven = isEven(a);

if(isEven == true)
	print("Variable a is even");
else if(isEven == false)
	print("Variable a is not even");

func boolean isEven(int number){
	if(number mod 2 == 0)
		return true;
	else
		return false;
}

FINISH

